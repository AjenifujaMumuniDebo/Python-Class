class Restaurant:
    def __init__(self, restaurant_name, cuisine_type):
        self.restaurant_name=restaurant_name
        self.cuisine_type=cuisine_type

    def describe_restaurant(self):
        print(self.restaurant_name)
        print(self.cuisine_type)

    def open_restaurant(self):
            print(self.restaurant_name + " is open")

restaurant=Restaurant("Daybour dish", "Abula")
print(restaurant.restaurant_name)
print (restaurant.cuisine_type)
restaurant.describe_restaurant()
restaurant.open_restaurant()

restaurantOne=Restaurant("Mosh spice", "Apple meals")
restaurantTwo=Restaurant("Ancestor meals", "Iyan")
restaurantThree=Restaurant("Afo International", "Oha special")

restaurantOne.describe_restaurant()
restaurantTwo.describe_restaurant()
restaurantThree.describe_restaurant()
